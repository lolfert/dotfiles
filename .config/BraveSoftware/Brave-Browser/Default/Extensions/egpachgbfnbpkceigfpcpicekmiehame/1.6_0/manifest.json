{
   "author": "Lorenzo Zottar",
   "background": {
      "persistent": true,
      "scripts": [ "js/bg.js" ]
   },
   "default_locale": "en",
   "description": "Pinterest Enhanced Experience - Fixed Height | Full Size images Download | Google Image Search | Arrow Keys Navigation through pins",
   "icons": {
      "64": "img/iconON.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_title__",
   "options_page": "options.html",
   "page_action": {
      "default_icon": "img/iconOFF.png",
      "default_title": "Pinterest Enhanced Extension"
   },
   "permissions": [ "tabs", "activeTab", "notifications", "*://*.pinterest.com/*" ],
   "short_name": "PinterestEnhanced",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.6",
   "web_accessible_resources": [ "css/fixedHeight.css", "css/extraDownload.css", "ajs/inject.js", "options.html", "img/download.png", "img/search.png" ]
}
